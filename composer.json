{
    "name": "wotz/filament-admin-bar",
    "description": "Admin bar for filament",
    "keywords": [
        "who owns the zebra",
        "laravel",
        "filament-admin-bar"
    ],
    "homepage": "https://github.com/wotzebra/filament-admin-bar",
    "license": "MIT",
    "support": {
        "email": "info@whoownsthezebra.be"
    },
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0|^5.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "livewire/livewire": "^3.2|^4.0",
        "spatie/laravel-package-tools": "^1.12",
        "wotz/filament-translatable-strings": "^2.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.0|^3.0",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0|^8.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.0|^3.0|^4.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0|^4.0",
        "phpstan/extension-installer": "^1.1|^2.0",
        "phpstan/phpstan-deprecation-rules": "^1.0|^2.0",
        "phpstan/phpstan-phpunit": "^1.0|^2.0"
    },
    "suggest":{
        "wotz/filament-seo": "^1.1 || ^2.0"
    },
    "autoload": {
        "psr-4": {
            "Wotz\\FilamentAdminBar\\": "src",
            "Wotz\\FilamentAdminBar\\Database\\Factories\\": "database/factories"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Wotz\\FilamentAdminBar\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wotz\\FilamentAdminBar\\Providers\\FilamentAdminBarServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
